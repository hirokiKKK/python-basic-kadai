        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def check_adult(self):\n",
        "    if self.age >= 20:\n",
        "      print(\"大人\")\n",
        "    else:\n",
        "      print(\"大人ではない\")\n",
        "\n",
        "taro = Human(\"太郎\", 30)\n",
        "hanako = Human(\"花子\", 10)\n",
        "\n",
        "taro.check_adult()\n",
        "hanako.check_adult()"
      ]
